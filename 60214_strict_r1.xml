	<!-- START Marginal Likelihood Estimator                                     -->
	<productOfExponentialsPosteriorMeansLoess id="exponentials" fileName="60214_strict_r1.log" burnin="10000000" parameterColumn="coalescentEventsStatistic" dimension="43">
